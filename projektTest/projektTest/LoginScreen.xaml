<Window x:Class="projektTest.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:projektTest"
        mc:Ignorable="d"
        Title="Logowanie" Height="300" Width="300" FontSize="15" Background="#336666">
    <StackPanel Margin="20">
        <Label Content="Login" Foreground="White" FontSize="25" HorizontalAlignment="Center"  VerticalAlignment="Top" />
        <Separator></Separator>
        <Label Content="Username" Foreground="White" HorizontalAlignment="Center"/>
        <TextBox Name="txtUsername" Background="#2F4F4F" FontSize="16" Foreground="White"/>
    
        <Label Content="Password" Foreground="White" HorizontalAlignment="Center"/>
        <TextBox Name="txtPass" Background="#2F4F4F" FontSize="16" Foreground="White"/>
        <Button Name="btnLogin" Content="Zalaguj" Margin="70 10"   Background="#2F4F4F" FontSize="16" Foreground="White" Click="BtnLogin_Click"/>
        <Button Content="Test" x:Name="btnTest" Click="BtnTest_Click"/>
    </StackPanel>
</Window>
